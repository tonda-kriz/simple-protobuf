syntax = "proto2";

//[[ string.type = "std::array<$, 6>"]]
//[[ string.include = "<array>"]]

//[[ bytes.type = "std::array<$, 8>"]]
//[[ bytes.include = "<array>"]]

package Test.Array@PB_PACKAGE@;

message OptString {
    optional string value = 1;
}

message ReqString {
    required string value = 1;
}

message RepString {
    repeated string value = 1;
}

message OptBytes {
    optional bytes value = 1;
}

message ReqBytes {
    required bytes value = 1;
}

message RepBytes {
    repeated bytes value = 1;
}
